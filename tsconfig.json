{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"baseUrl": ".",
	"paths": {
		"@components/*": ["src/components/*"],
		"@screens/*": ["src/screens/*"],
		"@data/*": ["src/data/*"],
		"@util/*": ["src/util/*"],
		"@static/*": ["src/static/*"],
		"@context/*": ["src/context/*"],
		"@native": ["src/GameNative.ts"]
	},
	"compilerOptions": {
		"skipLibCheck": true
	}
}